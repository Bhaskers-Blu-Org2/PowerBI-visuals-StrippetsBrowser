    "imports-loader": "^0.7.1",
    "json": "^9.0.6",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon-chai": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "memory-fs": "^0.4.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.5.0",
    "mv": "^2.1.1",
